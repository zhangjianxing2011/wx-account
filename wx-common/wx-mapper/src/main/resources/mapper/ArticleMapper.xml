<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.something.dao.mapper.ArticleMapper">

    <resultMap id="BaseResultMap" type="com.something.dao.domain.Article">
            <id property="id" column="id" />
            <result property="signId" column="sign_id" />
            <result property="title" column="title" />
            <result property="titleImg" column="title_img" />
            <result property="content" column="content" />
            <result property="contentImgs" column="content_imgs" />
            <result property="draftId" column="draft_id" />
            <result property="createdAt" column="created_at" />
            <result property="updatedAt" column="updated_at" />
            <result property="isDel" column="is_del" />
    </resultMap>

    <sql id="Base_Column_List">
        id,sign_id,title,title_img,content,content_imgs,
        draft_id,created_at,updated_at,is_del
    </sql>
</mapper>
